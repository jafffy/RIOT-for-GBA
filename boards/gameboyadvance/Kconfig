config BOARD
    default "gameboyadvance" if BOARD_GAMEBOYADVANCE

config BOARD_GAMEBOYADVANCE
    bool
    default y
    select HAS_TINYUSB_DEVICE
    select BOARD_COMMON_GAMEBOYADVANCE

source "$(RIOTBOARD)/common/gameboyadvance/Kconfig"
